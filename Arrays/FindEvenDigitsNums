public class FindEvenDigitsNums {
    public int findNumbers(int[] nums) {
        int digits = 1;
        int result = 0;
        for (int i = 0; i < nums.length; i++) {
            while (nums[i] / 10 > 0) {
                nums[i] = nums[i] / 10;
                digits++;
            }
            if (digits % 2 == 0) {
                result++;
            }
            digits = 1;
        }
        return result;
    }
}


//[12,345,2,6,7896]